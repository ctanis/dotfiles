;(package-initialize)

;; emacs 27.1 deprecates 'cl; this clears the warning triggered by many 3rd
;; party packages
(setq byte-compile-warnings '(cl-functions))


(add-to-list 'load-path "~/.dotfiles/elisp/ctanis")
(add-to-list 'load-path "~/.dotfiles/elisp/other")

(load-library "init")

(setq custom-file "~/.emacs.d/custom.el")
(if (file-readable-p custom-file)
    (load-file custom-file))

(if (file-readable-p "~/.emacs.d/projects.el")
	(load-file "~/.emacs.d/projects.el"))

(if (file-readable-p "~/.emacs-local")
    (load-file "~/.emacs-local"))
