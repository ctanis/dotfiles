set -g default-terminal "xterm-256color"
# 
set -g xterm-keys on
set -s extended-keys always
#set -g extended-keys-format csi-u
set -as terminal-features 'xterm*:extkeys'


unbind C-b
set -g prefix C-]
bind C-] send-prefix

set -g status off
set -g set-titles on
set -g set-titles-string '(#I:#W) #T'
set -g allow-rename on
